import turtle
wn = turtle.Screen()
wn.bgcolor("lightgreen")

alex = turtle.Turtle()
alex.color("blue")
alex.shape("turtle")

alex.up()
for i in range(12):
    alex.forward(50)
    alex.down()
    alex.forward(10)
    alex.up()
    alex.forward(10)
    alex.stamp()
    alex.backward(70)
    alex.left(360/12)
